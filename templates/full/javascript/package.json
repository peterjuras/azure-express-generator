{
  "name": "<%= appname %>",
  "version": "0.0.0",
  "scripts": {
    "start": "node src/server/server.js",
    "test": "npm run test-client && npm run test-server",
    "test-client": "mocha test/client/ --compilers js:babel-register --require=env-test",
    "test-server": "mocha test/server/ --require=env-test",
    "postinstall": "jspm install"
  },
  "dependencies": {
    "express": "^4.13.3",
    "express-react-views": "^0.10.0",
    "morgan": "^1.6.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.5.1",
    "chokidar-socket-emitter": "^0.4.2",
    "del": "^2.1.0",
    "env-test": "^1.0.0",
    "gulp": "^3.9.0",
    "gulp-filter": "3.0.1",
    "gulp-html-replace": "^1.5.5",
    "gulp-htmlmin": "^1.3.0",
    "gulp-install": "^0.6.0",
    "gulp-json-editor": "^2.2.1",
    "gulp-jspm": "^0.5.5",
    "gulp-mocha": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-strip-debug": "^1.1.0",
    "gulp-util": "^3.0.7",
    "jspm": "^0.16.15",
    "mkpath": "^1.0.0",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.3",
    "should": "^8.2.2",
    "superagent": "^1.4.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "src/client/app"
    },
    "devDependencies": {
      "react": "npm:react@^0.14.3",
      "react-dom": "npm:react-dom@^0.14.3",
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "capaj/systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@^0.5.5",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
